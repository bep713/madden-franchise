{
  "name": "madden-franchise",
  "version": "4.1.4",
  "description": "Tools to read and parse a madden franchise file",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run make-types && rollup -c",
    "format": "prettier . --write",
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix",
    "make-types": "tsc --project ./tsconfig.json",
    "pack": "npm pack --pack-destination pack",
    "publish": "npm run lint && npm run format && npm run build && npm publish",
    "test": "mocha tests/**/*.spec.js"
  },
  "keywords": [
    "madden",
    "franchise"
  ],
  "files": [
    "dist/index.cjs",
    "dist/index.mjs",
    "dist/*.d.ts",
    "dist/*.map",
    "data/**/*"
  ],
  "author": "matthewpanetta",
  "license": "MIT",
  "dependencies": {
    "bit-buffer": "^0.2.5",
    "fast-xml-parser": "^5.2.5",
    "node-xml-stream-parser": "^1.0.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bep713/madden-franchise.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "chai": "^5.2.1",
    "cjstoesm": "^3.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "mocha": "^9.1.4",
    "prettier": "3.6.2",
    "proxyquire": "^2.1.3",
    "quibble": "^0.9.2",
    "rollup": "^4.49.0",
    "sinon": "^21.0.0",
    "typescript": "^5.4.4"
  },
  "type": "module",
  "engines": {
    "node": ">=22.19.0"
  }
}
